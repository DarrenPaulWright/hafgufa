{
	"name": "hafgufa",
	"version": "0.12.2",
	"description": "A javascript ES6+ Frameworkless UI library.",
	"author": {
		"name": "Darren Paul Wright",
		"email": "Darren@DarrenPaulWright.com",
		"url": "https://www.darrenpaulwright.com"
	},
	"license": "MIT",
	"main": "index.js",
	"files": [
		"src",
		"localization"
	],
	"sideEffects": [
		"*.less"
	],
	"browserslist": [
		"defaults"
	],
	"scripts": {
		"test": "karma start --single-run",
		"test-watch": "karma start",
		"bench": "karma start karma.bench.conf.js",
		"start": "webpack-dev-server --open --config webpack.dev.js",
		"build": "webpack --config webpack.prod.js",
		"preversion": "npm test && npm run docs && git add --all && git diff --staged --quiet || git commit -m \"Updating docs\"",
		"postversion": "npm publish",
		"postpublish": "git push --follow-tags",
		"pre-clean": "rm -rf node_modules && del /f package-lock.json",
		"clean": "npm run pre-clean && npm install",
		"update": "ncu -u && npm install",
		"update-clean": "npm run pre-clean && npm run update",
		"update-commit": "npm run update && npm test && git add --all && git diff --staged --quiet || git commit -m \"Updating dependencies\"",
		"update-patch": "npm run update-commit && npm version patch",
		"docs": ""
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/DarrenPaulWright/hafgufa.git"
	},
	"keywords": [
		"UI",
		"UX"
	],
	"engines": {
		"node": "9.5.0"
	},
	"bugs": {
		"url": "https://github.com/DarrenPaulWright/object-agent/issues"
	},
	"homepage": "https://github.com/DarrenPaulWright/object-agent#readme",
	"dependencies": {
		"async-agent": "^0.5.3",
		"axios": "^0.19.2",
		"bowser": "^2.9.0",
		"d3": "^5.15.1",
		"date-fns": "^2.11.1",
		"get-browser-language": "^1.0.0",
		"hammerjs": "^2.0.8",
		"hord": "^0.7.1",
		"keycodes": "^1.0.0",
		"less": "^3.11.1",
		"object-agent": "^1.4.12",
		"pathinator": "^0.3.0",
		"shortid": "^2.2.15",
		"type-enforcer-ui": "^1.0.6"
	},
	"devDependencies": {
		"@babel/polyfill": "^7.8.7",
		"autoprefixer": "^9.7.5",
		"babel-polyfill": "^6.26.0",
		"clean-webpack-plugin": "^3.0.0",
		"copy-webpack-plugin": "^5.1.1",
		"html-webpack-plugin": "^4.0.4",
		"karma-webpack-bundle": "^0.4.1",
		"less-themes-webpack-plugin": "^1.3.8",
		"simulant": "^0.2.2",
		"uglifyjs-webpack-plugin": "^2.2.0",
		"webpack-cli": "^3.3.11",
		"webpack-dev-server": "^3.10.3",
		"webpack-merge": "^4.2.2"
	}
}
